{
	"name": "demo-api",
	"version": "0.0.0",
	"module": "src/index.js",
	"dependencies": {
		"@elysiajs/cors": "^1.4.0",
		"@elysiajs/openapi": "^1.4.11",
		"better-auth": "^1.3.34",
		"drizzle-orm": "^0.44.7",
		"drizzle-zod": "^0.8.3",
		"elysia": "1.4.13",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"@tsconfig/bun": "^1.0.9",
		"@types/bun": "1.3.1",
		"drizzle-kit": "^0.31.6",
		"globals": "^16.4.0",
		"openapi-types": "^12.1.3",
		"oxlint": "^1.24.0",
		"oxlint-tsgolint": "^0.3.0",
		"typescript": "^5.9.3"
	},
	"private": true,
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "bun run --watch src/index.ts",
		"lint": "oxlint --type-aware",
		"lint:fix": "oxlint --type-aware --fix",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit",
		"auth:generate": "bunx --bun @better-auth/cli@latest generate --config src/modules/auth/index.ts --output src/db/schema/auth.ts",
		"db:generate": "bun drizzle-kit generate --config drizzle.config.ts",
		"db:migrate": "bun run migrate.ts"
	},
	"type": "module"
}
