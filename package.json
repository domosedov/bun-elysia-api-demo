{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write . --ignore-path .prettierignore",
    "typecheck": "tsc --noEmit",
    "auth:generate": "bun node_modules/@better-auth/cli/dist/index.mjs generate --config src/modules/auth/index.ts --output src/db/schema/auth.ts",
    "db:generate": "bun drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "bun run migrate.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "better-auth": "^1.3.28",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "elysia": "latest",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.28",
    "@eslint/js": "^9.38.0",
    "@tsconfig/bun": "^1.0.9",
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "module": "src/index.js",
  "type": "module",
  "private": true
}
